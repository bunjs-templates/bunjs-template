{
  "name": "@bunjs-templates/bunjs-template",
  "version": "1.0.0",
  "private": true,
  "description": "A modern BunJS template using BiomeJS for linting/formatting",
  "type": "module",
  "scripts": {
    "dev": "bun run index.ts",
    "build": "bun build index.ts",
    "test": "bun test",
    "lint": "biome check .",
    "format": "biome format --write .",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@types/bun": "1.3.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
